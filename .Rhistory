qsort <-  function(a) {
qsort_kernel <- function(lo, hi,num) {
i <-  lo
j <-  hi
while (i < hi) { #while we haven't reached the top index
pivot <- a[floor((lo+hi)/2)] #choose the pivot about halfway through
while (i <= j) {
while (a[i] < pivot) {
i <- i + 1
num <- num+1
}
while (a[j] > pivot) {
num <- num +1
j <- j - 1
}
if (i <= j) {
t <- a[i]
a[i] <<- a[j]
a[j] <<- t
i <- i + 1;
j <- j - 1;
}
}
if (lo < j) {
num <- num + qsort_kernel(lo, j,0)
}
lo <- i
j <- hi
}
return(num)
}
num <-  qsort_kernel(1, length(a),0)
return(num)
}
sortperf = function(n) {
v = runif(n)
Cn <- qsort(v)
Xn <- Cn - 2*(n+1)/n -4*n
return(Xn)
}
